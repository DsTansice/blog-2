// 加载字体
@font-face
  font-family "harmony"
  src url('https://npm.elemecdn.com/@kmar/fonts@1.0.2/text/HarmonyOS.min.woff2') format('woff2'),
  url('https://npm.elemecdn.com/@kmar/fonts@1.0.2/text/HarmonyOS.min.woff') format('woff'),
  url('https://npm.elemecdn.com/@kmar/fonts@1.0.2/text/HarmonyOS.min.ttf') format('truetype')

@font-face
  font-family "JetBrains Mono"
  src url('https://npm.elemecdn.com/@kmar/fonts@1.0.0/text/jet.woff2') format('woff2'),
  url('https://npm.elemecdn.com/@kmar/fonts@1.0.0/text/jet.woff') format('woff'),
  url('https://npm.elemecdn.com/@kmar/fonts@1.0.0/text/jet.ttf') format('truetype')

blockquote
  font-family "JetBrains Mono"