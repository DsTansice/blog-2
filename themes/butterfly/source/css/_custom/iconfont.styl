// 加载字体
@font-face
  font-family "iconfont"
  font-display auto
  src url('https://npm.elemecdn.com/@kmar/fonts@1.0.0/icon/font.woff2') format('woff2'),
  url('https://npm.elemecdn.com/@kmar/fonts@1.0.0/icon/font.woff') format('woff'),
  url('https://npm.elemecdn.com/@kmar/fonts@1.0.0/icon/font.ttf') format('truetype')

.iconfont
  font-family "iconfont" !important
  font-size 1.25em
  font-style normal
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

